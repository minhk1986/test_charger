AA,CADA,GAIA,YAAA,CAAA;AACA,IAAA,aADA,yBACA,KADA,EACA;AAAA;;AACA,UAAA,GAAA,GAAA,KAAA,CAAA,IAAA,CAAA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,EAAA,KAAA,KAAA,CAAA,eAAA;AAAA,OAAA,CAAA;AACA,aAAA,GAAA,IAAA,IAAA,GAAA,GAAA,CAAA,aAAA,GAAA,EAAA;AACA;AAJA,GAAA,CAJA,CAtBA;AAiCA,EAAA,OAjCA,qBAiCA;AACA,QAAA,KAAA,aAAA,IAAA,IAAA,IAAA,KAAA,aAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,WAAA,SAAA,GAAA,KAAA;AACA;AACA,GArCA;AAsCA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,sBACA,IADA,EACA;AACA,WAAA,UAAA,GAAA,IAAA;AACA,WAAA,UAAA,GAAA,IAAA;AACA,KAJA;AAKA,IAAA,aALA,yBAKA,KALA,EAKA;AACA,cAAA,KAAA;AACA,aAAA,CAAA;AACA,iBAAA,gBAAA;;AACA,aAAA,CAAA;AACA,iBAAA,gBAAA;;AACA,aAAA,CAAA;AACA,iBAAA,cAAA;;AACA,aAAA,CAAA;AACA,iBAAA,oBAAA;;AACA;AACA,iBAAA,OAAA;AAVA;AAYA,KAlBA;AAmBA,IAAA,YAnBA,wBAmBA,KAnBA,EAmBA;AACA,cAAA,KAAA;AACA,aAAA,CAAA;AACA,iBAAA,0BAAA;;AACA,aAAA,CAAA;AACA,iBAAA,WAAA;;AACA,aAAA,CAAA;AACA,iBAAA,kBAAA;;AACA,aAAA,CAAA;AACA,iBAAA,kBAAA;;AACA;AACA,iBAAA,EAAA;AAVA;AAYA;AAhCA,GAtCA;AAwEA,EAAA,KAAA,EAAA;AACA,IAAA,aADA,yBACA,GADA,EACA;AACA,UAAA,GAAA,IAAA,IAAA,IAAA,GAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,aAAA,SAAA,GAAA,KAAA;AACA;AACA,KALA;AAMA,IAAA,UANA,sBAMA,GANA,EAMA;AACA,UAAA,GAAA,KAAA,KAAA,EAAA;AACA,aAAA,UAAA,GAAA;AACA,UAAA,KAAA,EAAA,CADA;AAEA,UAAA,IAAA,EAAA,EAFA;AAGA,UAAA,OAAA,EAAA,EAHA;AAIA,UAAA,WAAA,EAAA,EAJA;AAKA,UAAA,MAAA,EAAA;AALA,SAAA;AAOA;AACA;AAhBA;AAxEA,CAAA","sourcesContent":["<template>\n  <v-container fluid>\n    <v-card elevation=\"6\">\n      <v-card-title>Status devices</v-card-title>\n      <v-data-table\n        :headers=\"headers\"\n        :items=\"statusDevices\"\n        :items-per-page=\"5\"\n        :search=\"search\"\n        loading-text=\"Loading... Please wait\"\n        :loading=\"isLoading\"\n      >\n        <template v-slot:top>\n          <v-row no-gutters>\n            <v-spacer />\n            <v-col xs=\"12\" sm=\"8\" md=\"6\" lg=\"4\">\n              <v-text-field v-model=\"search\" label=\"Search\" class=\"mx-4\" />\n            </v-col>\n          </v-row>\n        </template>\n        <template v-slot:[`item.name`]=\"{ item }\">\n          <v-icon\n            v-text=\"getIconLevel(item.level)\"\n            :color=\"getColorLevel(item.level)\"\n          ></v-icon>\n          <span>{{ item.name }}</span>\n        </template>\n        <template v-slot:[`item.actions`]=\"{ item }\">\n          <v-icon\n            @click=\"() => viewValues(item)\"\n            :color=\"getColorLevel(item.level)\"\n          >\n            mdi-view-list-outline\n          </v-icon>\n        </template>\n      </v-data-table>\n    </v-card>\n    <v-dialog v-model=\"dialogShow\" max-width=\"800\">\n      <v-card>\n        <v-card-title\n          :class=\"`headline white--text ${getColorLevel(dialogItem.level)}`\"\n        >\n          {{ dialogItem.name }}\n        </v-card-title>\n        <v-card-subtitle\n          :class=\"`py-2 white--text ${getColorLevel(dialogItem.level)}`\"\n          >{{ dialogItem.hardware_id }}</v-card-subtitle\n        >\n        <v-card-text>{{ dialogItem.message }}</v-card-text>\n        <v-simple-table class=\"mt-2\" height=\"400px\">\n          <tbody>\n            <tr v-for=\"item in dialogItem.values\" :key=\"item.name\">\n              <td>{{ item.key }}</td>\n              <td>{{ item.value }}</td>\n            </tr>\n          </tbody>\n        </v-simple-table>\n      </v-card>\n    </v-dialog>\n  </v-container>\n</template>\n<script>\nimport { createNamespacedHelpers } from \"vuex\";\n\nconst { mapState: navigationMapState, } = createNamespacedHelpers(\"navigation\");\nconst { mapState: agvsMapState } = createNamespacedHelpers(\"agvs\");\n\nexport default {\n  name: \"AgvrStatusDevices\",\n  data() {\n    return {\n      search: \"\",\n      isLoading: true,\n      headers: [\n        { text: \"Componet\", value: \"name\" },\n        { text: \"Hardware ID\", value: \"hardware_id\" },\n        { text: \"Message\", value: \"message\" },\n        { text: \"\", value: \"actions\", sortable: false, width: 50 },\n      ],\n      dialogShow: false,\n      dialogItem: {\n        level: 0,\n        name: \"\",\n        message: \"\",\n        hardware_id: \"\",\n        values: [],\n      },\n    };\n  },\n  computed: {\n    ...navigationMapState({\n      navigationAgvId: (state) => state.agvId,\n    }),\n    ...agvsMapState({\n      statusDevices(state) {\n        let agv = state.find(a => a.id === this.navigationAgvId);\n        return agv != null ? agv.statusDevices : [];\n      }\n    }),\n  },\n  mounted() {\n    if (this.statusDevices != null && this.statusDevices.length > 0) {\n      this.isLoading = false;\n    }\n  },\n  methods: {\n    viewValues(item) {\n      this.dialogItem = item;\n      this.dialogShow = true;\n    },\n    getColorLevel(level) {\n      switch (level) {\n        case 0:\n          return \"green darken-1\";\n        case 1:\n          return \"amber darken-1\";\n        case 2:\n          return \"red darken-1\";\n        case 3:\n          return \"blue-grey darken-1\";\n        default:\n          return \"black\";\n      }\n    },\n    getIconLevel(level) {\n      switch (level) {\n        case 0:\n          return \"mdi-check-circle-outline\";\n        case 1:\n          return \"mdi-alert\";\n        case 2:\n          return \"mdi-minus-circle\";\n        case 3:\n          return \"mdi-close-circle\";\n        default:\n          return \"\";\n      }\n    },\n  },\n  watch: {\n    statusDevices(val) {\n      if (val != null && val.length > 0) {\n        this.isLoading = false;\n      }\n    },\n    dialogShow(val) {\n      if (val === false) {\n        this.dialogItem = {\n          level: 0,\n          name: \"\",\n          message: \"\",\n          hardware_id: \"\",\n          values: [],\n        };\n      }\n    },\n  },\n};\n</script>"],"sourceRoot":"src/components/Overviews/AgvrStatusDevices"}]}